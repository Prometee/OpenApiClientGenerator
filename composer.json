{
    "name": "prometee/openapi-client-generator",
    "description": "Open API PHP8 skeleton client generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ],
    "require": {
        "ext-iconv": "*",
        "ext-json": "*",
        "php": "^8.1",
        "prometee/php-class-generator": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "rector/rector": "^1.1",
        "symplify/easy-coding-standard": "^12.1",
        "phpstan/phpstan": "^1.11"
    },
    "autoload": {
        "psr-4": { "Prometee\\SwaggerClientGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Prometee\\SwaggerClientGenerator\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
