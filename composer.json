{
    "name": "prometee/swagger-client-generator",
    "description": "Swagger PHP7 client generator",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "prometee@gmail.com"
        }
    ],
    "require": {
        "ext-iconv": "*",
        "ext-json": "*",
        "php": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": { "Prometee\\SwaggerClientGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\Prometee\\SwaggerClientGenerator\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
